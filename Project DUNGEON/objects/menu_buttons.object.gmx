//Check if the button is being pressed

    if((!instance_exists(Fade_Box)) and global.touch_button and click == 1){selected = 1;}
        if(selected == 1 and global.release_button){
            selected = 0;
            
            if(mode == "start" and oScaleControl.menu_mode == "menu"){
                //Start prepping for new game
                if(image_index == 0){
                    oScaleControl.menu_mode = "class"
                    //Open up the class selection menu, which is just three buttons 
                    class_button_1 = instance_create(0,0,menu_buttons);
                    class_button_1.sprite_index = class_select_spr;
                    class_button_1.mode = "class select";
                    class_button_1.image_index = 0;
                    class_button_1.depth = depth-1;
                    
                    class_button_2 = instance_create(107,0,menu_buttons);
                    class_button_2.sprite_index = class_select_spr;
                    class_button_2.mode = "class select";
                    class_button_2.image_index = 1;
                    class_button_2.depth = depth-1;
                    
                    class_button_3 = instance_create(214,0,menu_buttons);
                    class_button_3.sprite_index = class_select_spr;
                    class_button_3.mode = "class select";
                    class_button_3.image_index = 2;
                    class_button_3.depth = depth-1;
                }
                //Go to options menu
                else if(image_index == 1){
                    room_goto(Options);
                }
                //Go to Library
                else if(image_index &gt;= 2){
                    room_goto(Library_room);
                }
            }
            
            else if(mode == "class select" and !instance_exists(Fade_Box)){
                instance_create(0,0,Fade_Box);
                audio_sound_gain(op_song,0,1500);
                //Select Knight
                if(image_index == 0){
                    global.player_class = "Knight";    
                }
                else if(image_index == 1){
                    global.player_class = "Mage";
                }
                else if(image_index == 2){
                    global.player_class = "Rogue";
                }
            }
        }
    }
</string>
