if(Stat_Screen.moving == false and Text_Box.display_log == false and Dungeon.init == false){

    //Check if the screen is being pressed
    if(activated == true or keyboard_check_pressed(vk_backspace)){
        selected = 0;
        activated = false;
        if(active == false){
            active = true;
            moving = true;
        }
        
        var str = Dungeon.player_1.strength;
        var mag = Dungeon.player_1.magic;
        var def = Dungeon.player_1.defense;
        var res = Dungeon.player_1.res;
        var lck = Dungeon.player_1.luck;
        
        //Reset the stat digits
        if(str &lt; 10){
            str_1 = 0;
            str_2 = str;
        }
        else{
            str_1 = floor(str/10);
            str_2 = (str-(str_1*10));
        }
        
        if(mag &lt; 10){
            mag_1 = 0;
            mag_2 = mag;
        }
        else{
            mag_1 = floor(mag/10);
            mag_2 = (mag-(mag_1*10));
        }
        
        if(def &lt; 10){
            def_1 = 0;
            def_2 = def;
        }
        else{
            def_1 = floor(def/10);
            def_2 = (def-(def_1*10));
        }
        
        if(res &lt; 10){
            res_1 = 0;
            res_2 = res;
        }
        else{
            res_1 = floor(res/10);
            res_2 = (res-(res_1*10));
        }
        
        if(lck &lt; 10){
            lck_1 = 0;
            lck_2 = lck;
        }
        else{
            lck_1 = floor(lck/10);
            lck_2 = (lck-(lck_1*10));
        }
        
        
        
        
    }


}


}
