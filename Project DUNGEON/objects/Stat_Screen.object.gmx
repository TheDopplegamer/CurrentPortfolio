    }
    else{draw_sprite(stat_number_spr,mag_2,x+191,y+50);}

    if(def_1 &gt; 0){
        draw_sprite(stat_number_spr,def_1,x+187,y+90);
        draw_sprite(stat_number_spr,def_2,x+195,y+90);
    }
    else{draw_sprite(stat_number_spr,def_2,x+191,y+90);}
    
    if(res_1 &gt; 0){
        draw_sprite(stat_number_spr,res_1,x+187,y+130);
        draw_sprite(stat_number_spr,res_2,x+195,y+130);
    }
    else{draw_sprite(stat_number_spr,res_2,x+191,y+130);}
    
    if(lck_1 &gt; 0){
        draw_sprite(stat_number_spr,lck_1,x+187,y+170);
