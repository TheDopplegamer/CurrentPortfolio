    }
    else{draw_sprite(stat_number_spr,lck_2,x+191,y+170);}    
    
    
    //Draw stat bar
    var ds = 0;
    var dx = 0;
    while(ds &lt; 99){
        if(ds &lt; Dungeon.player_1.strength){
            draw_sprite(stat_bar_spr,0,x+dx+214,y+12);
        }
        if(ds &lt; Dungeon.player_1.magic){
            draw_sprite(stat_bar_spr,1,x+dx+214,y+52);
        }
        if(ds &lt; Dungeon.player_1.defense){
            draw_sprite(stat_bar_spr,2,x+dx+214,y+92);
        }
        if(ds &lt; Dungeon.player_1.res){
            draw_sprite(stat_bar_spr,3,x+dx+214,y+132);
        }
        if(ds &lt; Dungeon.player_1.luck){
            draw_sprite(stat_bar_spr,4,x+dx+214,y+172);
        }
        
    
        ds += 1;
        dx += 1;
    }
    
    
}

