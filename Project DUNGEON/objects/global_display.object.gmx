    if(s == iap_status_available){
        var st = "Available";
    }
    else if(s == iap_status_loading){
        var st = "Loading";
    }
    else if(s == iap_status_processing){
        var st = "Processing";
    }
    else if(s == iap_status_restoring){
        var st = "Restoring";
    }
    else if(s == iap_status_unavailable){
        var st = "Unavailable";
    }
    else if(s == iap_status_uninitialised){
        var st = "Uninitialized";
    }
    else{
        var st = "???";
    }
    
    if(global.ads == true){
        var at = "Active";
    }
    else{
        var at = "Inactive";
    }
    
    //Draw global variables
    draw_text(40,40,"Store Status: " + st);
    draw_text(40,80,"Ads Status: " + at);
    
    if(s == iap_status_available){
        var p_status = ds_map_find_value(contents,"status");
        var p_id = ds_map_find_value(contents,"product");
        
        if(p_status == iap_available){
            var ds = "Available";
        }
        else if(p_status == iap_failed){
            var ds = "Failed";
        }
        else if(p_status == iap_purchased){
            var ds = "Purchased";
        }
        else if(p_status == iap_canceled){
            var ds = "Canceled";
        }
        else if(p_status == iap_refunded){
            var ds = "Refunded";
        }   
        else if(p_status == iap_unavailable){
            var ds = "Unavailable";
        }
        else{
            var ds = "???";
        }
        
        draw_text(40,120,"Purchase Status: " + ds);
        draw_text(40,160,"Product ID: " + p_id);
    }
    


}




