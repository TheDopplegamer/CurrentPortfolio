        audio_stop_all();
        Save_Progress();
        oScaleControl.menu_mode = "menu";
        if(type == "death"){room_goto(Ending_Room);}
        else if(type == "main"){room_goto(Title_Screen);}
        else if(type == "endless"){room_goto(endless_dungeon_room);}
        else if(type == "results"){room_goto(endless_results_screen);}
        else{room_goto(dungeon_room);}
        }
